{
  "name": "@funstack/static",
  "version": "0.0.1",
  "description": "FUNSTACK static library",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/entries/server.d.mts",
      "import": "./dist/entries/server.mjs"
    },
    "./entries/*": "./dist/entries/*.mjs"
  },
  "imports": {
    "#rsc-client": {
      "types": "./src/entries/rsc-client.ts",
      "default": "./dist/entries/rsc-client.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "author": "uhyo <uhyo@uhy.ooo>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "jsdom": "^27.3.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tsdown": "^0.19.0",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@vitejs/plugin-rsc": "^0.5.14",
    "react-error-boundary": "^6.0.3",
    "rsc-html-stream": "^0.0.7",
    "srvx": "^0.10.0"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "vite": "catalog:"
  }
}
